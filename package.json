{
  "name": "panda-9000",
  "version": "1.0.0-alpha-03",
  "description": "Task and build scripts using functional reactive programming",
  "main": "lib/index.js",
  "files": [
    "lib",
    "bin",
    "package.json",
    "README.md",
    "VERSION",
    "LICENSE"
  ],
  "bin": {
    "p9k": "bin/p9k"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dyoder/panda-9000.git"
  },
  "scripts": {
    "prepublish": "coffee --nodejs --harmony -o lib/ -c src/*.*coffee",
    "test": "coffee --nodejs --harmony test/index.coffee",
    "watch": "coffee --nodejs --harmony -o lib/ -cw src/*.*coffee",
    "version": "(node_modules/.bin/json -f package.json version > VERSION)",
    "tag": "(cat VERSION | xargs -I version git tag -am version version) && git push --tags"
  },
  "keywords": [
    "build",
    "task",
    "gulp",
    "grunt",
    "functional",
    "reactive"
  ],
  "author": "Dan Yoder",
  "license": "ISC",
  "readmeFilename": "README.md",
  "dependencies": {
    "fairmont": "^1.0.0-beta-29",
    "jade": "^1.11.0",
    "marked": "^0.3.5",
    "stylus": "^0.52.0",
    "coffee-script": "^1.9.3",
    "when": "^3.7.3"
  },
  "engine": "node >= 0.12.x",
  "devDependencies": {
    "json": "^9.0.3"
  }
}
